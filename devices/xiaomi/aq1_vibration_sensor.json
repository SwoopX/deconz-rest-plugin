{
  "schema": "devcap1.schema.json",
  "manufacturername": "$MF_LUMI",
  "modelid": "lumi.vibration.aq1",
  "vendor": "Xiaomi",
  "product": "Aqara Vibration Sensor DJT11LM",
  "status": "Gold",
  "sleeper": true,
  "md:known_issues": [ "xiaomi_known_issues1.md" ],
  "subdevices": [
    {
      "type": "$TYPE_VIBRATION_SENSOR",
      "restapi": "/sensors",
      "uuid": [
        "$address.ext",
        "0x01",
        "0x0101"
      ],
      "fingerprint": {
        "profile": "0x0104",
        "device": "0x000a",
        "endpoint": "0x01",
        "in": ["0x0101"]
      },
      "items": [
        {
          "name": "attr/lastseen"
        },
        {
          "name": "attr/manufacturername"
        },
        {
          "name": "attr/modelid",
          "awake": true
        },
        {
          "name": "attr/name"
        },
        {
          "name": "attr/swversion",
          "awake": true,
          "parse": {
            "at": "0xff01",
            "ep": 1,
            "fn": "xiaomi:special",
            "idx": "0x08",
            "script": "xiaomi_firmware.js"
          },
          "read": {
            "fn": "none"
          }
        },
        {
          "name": "attr/type"
        },
        {
          "name": "attr/uniqueid"
        },
        {
          "name": "config/autoreset",
          "static": true,
          "public": false
        },
        {
          "name": "config/battery",
          "awake": true,
          "parse": {
            "at": "0xff01",
            "fn": "xiaomi:special",
            "idx": "0x01",
            "script": "xiaomi_battery.js"
          }
        },
        {
          "name": "config/duration",
          "default": 65
        },
        {
          "name": "config/on"
        },
        {
          "name": "config/sensitivity",
          "refresh.interval": 86400,
          "read": {
            "at": "0xff0d",
            "cl": "0x0000",
            "ep": 1,
            "fn": "zcl",
            "mf": "0x115f"
          },
          "parse": {
            "at": "0xff0d",
            "cl": "0x0000",
            "ep": 1,
            "eval": "Item.val = [3, 2, 1][Attr.val - 1]",
            "fn": "zcl",
            "mf": "0x115f"
          },
          "write": {
            "at": "0xff0d",
            "cl": "0x0000",
            "dt": "0x20",
            "ep": 1,
            "eval": "Item.val = [3, 2, 1][Item.val - 1]",
            "fn": "zcl",
            "mf": "0x115f"
          },
          "values": [
              [1, "low"], [2, "medium"], [3, "high"]
          ],
          "default": 3
        },
        {
          "name": "config/sensitivitymax",
          "default": 3
        },
        {
          "name": "state/orientation"
        },
        {
          "name": "state/orientation_x",
          "parse": {
            "at": "0x0508",
            "cl": "0x0101",
            "ep": 1,
            "fn": "zcl",
            "script": "aq1_vibration_sensor_orientation.js"
          }
        },
        {
          "name": "state/orientation_y"
        },
        {
          "name": "state/orientation_z"
        },
        {
          "name": "state/tiltangle",
          "parse": {
            "at": "0x0503",
            "cl": "0x0101",
            "ep": 1,
            "eval": "Item.val = Attr.val",
            "fn": "zcl"
          },
          "default": 0
        },
        {
          "name": "state/vibration",
          "parse": {
            "at": "0x0055",
            "cl": "0x0101",
            "ep": 1,
            "eval": "Item.val = Attr.val == 1 ? true : false",
            "fn": "zcl"
          },
          "default": false
        },
        {
          "name": "state/vibrationstrength",
          "parse": {
            "at": "0x0505",
            "cl": "0x0101",
            "ep": 1,
            "eval": "Item.val = Attr.val >> 16",
            "fn": "zcl"
          },
          "default": 0
        },
        {
          "name": "state/lastupdated"
        }
      ]
    }
  ]
}